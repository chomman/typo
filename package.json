{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run build:prod",
    "clean": "trash dist",
    "coverage": "npm run test -- --coverage",
    "coverage:watch": "npm run coverage -- --watch",
    "lint": "tsc && npm run lint:file",
    "lint:file": "tslint --project tsconfig.json",
    "test": "jest --config .jestrc.json",
    "test:watch": "npm run test -- --watch",
    "server": "cross-env NODE_ENV=development HOT_MODULES=true webpack-dev-server",
    "build:base": "npm run clean && webpack",
    "build:dev": "cross-env NODE_ENV=development npm run build:base",
    "build:prod": "cross-env NODE_ENV=production npm run build:base"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@jupl/tslint-config": "^0.5.1",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.3",
    "@types/webpack": "^4.4.17",
    "@types/webpack-dev-server": "^3.1.1",
    "@types/webpack-env": "^1.13.6",
    "babel-core": "^7.0.0-bridge.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "lint-staged": "^8.0.4",
    "normalize.css": "^8.0.0",
    "trash-cli": "^1.4.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-immutable": "^4.9.1",
    "tslint-sonarts": "^1.8.0",
    "typescript": "^3.1.6",
    "typescript-tslint-plugin": "0.0.5",
    "wcb": "^0.10.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
