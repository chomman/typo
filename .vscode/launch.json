{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Main",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "server:dev"
      ],
      "port": 9229
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "Debug Renderer",
      "port": 9230,
      "timeout": 30000
    }
  ],
  "compounds": [
    {
      "name": "Debug",
      "configurations": ["Debug Main", "Debug Renderer"]
    }
  ]
}
